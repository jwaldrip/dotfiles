{
  ".source.js.jsx": {
    "editor": {
      "commentStart": "// ",
      "foldEndPattern": "^\\s*\\}|^\\s*\\]|^\\s*\\)",
      "increaseIndentPattern": "{[^}\"']*$|\\[[^\\]\"']*$|\\([^)\"']*$",
      "decreaseIndentPattern": "^\\s*(\\s*/[*].*[*]/\\s*)*[}\\])]"
    }
  },
  ".meta.tag.jsx": {
    "editor": {
      "commentStart": "{/*",
      "commentEnd": "*/}"
    }
  }
}
